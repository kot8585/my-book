// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  userId String   
  email String?  @unique
  name  String?
  image String?
  type String @default("")
  createdAt DateTime @default(now())
  updatedAt DateTime?
  @@unique([userId, type])
}


